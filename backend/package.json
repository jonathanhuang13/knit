    "start": "npm run build && node -r module-alias/register dist/src/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "apollo-server": "^3.8.1",
    "dotenv": "^16.0.1",
    "firebase-admin": "^10.2.0",
    "graphql": "^16.5.0",
    "module-alias": "^2.2.2",
    "nexus": "^1.4.0-next.9",
    "p-map": "^5.4.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.4.2",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.2"
  },
  "_moduleAliases": {
    "@db": "dist/src/db",
    "@external": "dist/src/external"
